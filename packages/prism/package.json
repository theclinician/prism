{
  "name": "@theclinician/prism",
  "type": "module",
  "version": "0.0.6",
  "description": "Component library for Solid.js",
  "license": "MIT",
  "author": {
    "name": "subhog"
  },
  "repository": "github:theclinician/prism",
  "homepage": "https://github.com/theclinician/prism",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.css",
    "dist/*.js",
    "dist/*.jsx",
    "dist/*.ts",
    "dist/*.tsx",
    "dist/*.cjs",
    "dist/*.mjs",
    "dist/*.d.ts"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.jsx",
    "**/*.tsx"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "solid": "./dist/index.esm.js",
        "default": "./dist/index.esm.js",
        "import": "./dist/index.esm.js",
        "browser": "./dist/index.esm.js"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "source": "src/index.tsx",

  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "peerDependencies": {
    "solid-js": "1.8.15"
  },
  "devDependencies": {
    "sass": "1.71.1",
    "typescript": "5.3.3",
    "webpack":"5.90.3",
    "html-webpack-plugin": "5.6.0",
    "vite": "5.1.4",
    "vite-plugin-solid": "2.10.1",
    "vite-plugin-dts": "3.7.3"
  }
}
