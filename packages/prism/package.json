{
  "name": "@theclinician/prism",
  "type": "module",
  "version": "0.0.5",
  "description": "Component library for Solid.js",
  "license": "MIT",
  "author": {
    "name": "subhog"
  },
  "repository": "github:theclinician/prism",
  "homepage": "https://github.com/theclinician/prism",
  "main": "./dist/index/index.common.js",
  "module": "./dist/index/index.module.js",
  "types": "./dist/index/index.d.ts",
  "sideEffects": [
    "**/*.css",
    "**/*.jsx",
    "**/*.tsx"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index/index.d.ts",
        "solid": "./dist/index/index.module.js",
        "default": "./dist/index/index.module.js",
        "import": "./dist/index/index.module.js",
        "browser": "./dist/index/index.module.js"
      }
    },
    "./style.css": "./dist/index/index.module.css"
  },
  "source": "src/index.tsx",
  "files": [
    "dist/*.css",
    "dist/*.js",
    "dist/*.jsx",
    "dist/*.mjs",
    "dist/*.cjs",
    "dist/*.tsx",
    "dist/*.d.ts"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "rollup -c rollup.config.js",
    "build-vite": "tsc && vite build",
    "build-tsup": "tsup"
  },
  "dependencies": {
    "solid-js": "1.8.15"
  },
  "devDependencies": {
    "esbuild": "0.20.1",
    "sass": "1.71.1",
    "typescript": "5.3.3",
    "tsup": "8.0.2",
    "tsup-preset-solid": "2.2.0",
    "esbuild-plugin-solid": "0.5.0",
    "rollup": "4.12.0",
    "rollup-preset-solid": "2.0.1",
    "rollup-plugin-postcss": "4.0.2",
    "@rollup/plugin-alias": "5.1.0",
    "vite": "5.1.4",
    "vite-plugin-dts": "3.7.3",
    "vite-plugin-solid": "2.10.1",
    "postcss": "8.4.35"
  },
  "browser": {},
  "typesVersions": {
    "*": {}
  }
}
