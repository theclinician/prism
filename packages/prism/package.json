{
  "name": "@theclinician/prism",
  "type": "module",
  "version": "0.0.6",
  "description": "Component library for Solid.js",
  "license": "MIT",
  "author": {
    "name": "subhog"
  },
  "repository": "github:theclinician/prism",
  "homepage": "https://github.com/theclinician/prism",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.css",
    "dist/*.png",
    "dist/*.js",
    "dist/*.jsx",
    "dist/*.ts",
    "dist/*.tsx",
    "dist/*.cjs",
    "dist/*.mjs",
    "dist/*.d.ts"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.jsx",
    "**/*.tsx"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "solid": "./dist/index.js",
        "default": "./dist/index.js",
        "import": "./dist/index.js",
        "browser": "./dist/inde.js"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "source": "src/index.tsx",
  "scripts": {
    "dev": "vite build --watch",
    "build-vite": "vite build",
    "build": "webpack --mode production"
  },
  "peerDependencies": {
    "solid-js": "1.8.15"
  },
  "devDependencies": {
    "typescript": "5.3.3",
    "ts-loader": "9.5.1",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "sass": "1.71.1",
    "sass-loader": "14.1.1",
    "css-loader": "6.10.0",
    "style-loader": "3.3.4",
    "solid-js": "1.8.15",
    "file-loader": "6.2.0",
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "babel-loader": "9.1.3",
    "babel-preset-solid": "1.8.15",
    "html-webpack-plugin": "5.6.0",
    "vite": "5.1.4",
    "vite-plugin-solid": "2.10.1",
    "vite-plugin-dts": "3.7.3"
  }
}
